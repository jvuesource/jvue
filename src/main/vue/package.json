{
  "name": "jvue",
  "version": "1.0.3",
  "keywords": [
    "jvue",
    "vue",
    "java",
    "spring",
    "spring-boot",
    "webpack",
    "j2v8",
    "ssr",
    "server-side-rendering"
  ],
  "description": "Next light-weight,responsive project With Vue,webpack,Spring Boot and eclipse j2v8 Script engine for server-side-rendering",
  "main": "src/server.js",
  "author": "Terwer",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.vue,.html --ignore-path .gitignore --ignore-pattern !.eslintrc.js --ignore-pattern !.babelrc.js . --fix --color",
    "lint-css": "stylelint --config stylelint.config.js 'src/**/*.css' --fix --color",
    "serve": "cross-env NODE_ENV=development SSR_ENV=c webpack-dev-server --config build/webpack.client.config.js --progress --hide-modules",
    "build-entry-client-dev": "cross-env NODE_ENV=development SSR_ENV=ssrc webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build-entry-server-dev": "cross-env NODE_ENV=development SSR_ENV=ssrs webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build-dev": "./build/build.dev.sh",
    "dev": "yarn build-dev && yarn nodemon ./test/dev-server.js",
    "build-entry-client-prod": "cross-env NODE_ENV=production SSR_ENV=ssrc webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build-entry-server-prod": "cross-env NODE_ENV=production SSR_ENV=ssrs webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build": "./build/build.prod.sh",
    "prod": "yarn build && node ./test/dev-server.js",
    "start-dev": "nodemon ./test/dev-server.js",
    "start": "node ./test/v8-server.js",
    "test": "echo 'No Tests'"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.3.1",
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.13",
    "jsdom": "^13.2.0",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.6.6",
    "vue-uweb": "^0.2.1",
    "vue-web-storage": "^3.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.13.0",
    "eslint-config-google": "^0.12.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-vue": "^5.1.0",
    "express": "4.15.3",
    "express-thymeleaf": "^0.2.0",
    "extract-css-chunks-webpack-plugin": "^3.3.2",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "serve-favicon": "^2.5.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-no-browser-hacks": "^1.2.1",
    "thymeleaf": "^0.14.0",
    "url-loader": "^1.1.2",
    "vue-hot-reload-api": "^2.3.3",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
